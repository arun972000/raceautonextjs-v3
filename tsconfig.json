{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/HomeCategories/Varients/Varient 3/Varient3.jsx",
    "app/magazine/[title_slug]/FlipBook.jsx",
    "components/BootstrapClient.js",
    "app/api/admin/post/create/route.js",
    "app/api/admin/post/edit/[id]/route.js",
    "app/api/admin/magazine/[id]/route.js",
    "app/api/admin/magazine/route.js",
    "app/api/magazine-ad/route.js",
    "app/api/admin/event/[id]/route.js",
    "app/api/admin/event/route.js",
    "app/api/admin/event/settings/route.js",
    "app/api/admin/user/[id]/route.js",
    "app/api/login/route.js",
    "app/login/login.jsx",
    "app/register/signup.jsx",
    "app/api/admin/adspace/[title]/route.js",
    "app/api/admin/subscriber/upload-csv/route.js",
    "app/api/profile/[email]/route.js",
    "app/api/login/forgot-password/route.js",
    "app/api/login/verify-token/[token]/route.js",
    "app/verifytoken/[token]/ProtectedForm.jsx",
    "app/admin/components/Post/post-edit/EditPost.jsx",
    "app/admin/components/Post/PostCreate/Create.jsx",
    "app/admin/event/settings/settings.jsx",
    "app/admin/components/Dashboard/BarChart.jsx",
    "app/admin/settings/components/contactSettings.jsx",
    "app/api/admin/settings/logo/route.js",
    "app/admin/article/slider/page.tsx",
    "app/admin/article/slider/slider.jsx",
    "app/loader.js"
, "next-sitemap.config.js", "app/admin/article/breaking/Breaking.jsx"  ],
  "exclude": ["node_modules"]
}
